@model NhaHangTiecCuoi.Sanh

@{
    ViewBag.Title = "Delete";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}
<h3>Bạn có thật sự muốn xóa sảnh này không ?</h3>
<div>
    <hr />
    <img src="~/Content/photo/@Html.DisplayFor(model => model.Img)" width="550" />
    <dl class="dl-horizontal">
        <dt>
           Tên Sảnh 
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TenSanh)
        </dd>

        <dt>
            Giá Sảnh
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Gia)
        </dd>

    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Xóa Sảnh" class="btn btn-success" /> |
            <a href="http://localhost:50458/Admin/Sanhs" class="btn btn-danger">Hủy</a>
        </div>
    }
</div>
